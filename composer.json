{
  "name": "symfony-orchestra/view-bundle",
  "type": "library",
  "description": "The view symfony bundle for JSON API responses",
  "keywords": [
    "symfony",
    "view-bundle",
    "view",
    "api",
    "json",
    "typescript"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Lukin",
      "email": "lukin.andrej@gmail.com",
      "homepage": "https://github.com/wtorsi",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.4",
    "symfony/http-kernel": "8.0.*",
    "symfony/serializer": "8.0.*",
    "symfony/property-access": "8.0.*",
    "symfony/dependency-injection": "8.0.*",
    "symfony/config": "8.0.*",
    "doctrine/common": "^3.5",
    "symfony/framework-bundle": "^8.0",
    "symfony/runtime": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.5",
    "symfony/test-pack": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "ChamberOrchestra\\ViewBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "8.0.*"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
